# Локальная разработка
DEBUG=true
ENVIRONMENT=development
APP_NAME=Coworking API
APP_VERSION=1.0.0-dev

# Безопасность (для разработки)
SECRET_KEY=dev-secret-key-change-in-production
SECRET_KEY_JWT=dev-jwt-secret-key-change-in-production
ADMIN_LOGIN=admin
ADMIN_PASSWORD=admin

# Telegram Bot (тестовые значения)
BOT_TOKEN=your-test-bot-token
ADMIN_TELEGRAM_ID=your-admin-id
BOT_LINK=https://t.me/your_test_bot
INVITE_LINK=your-test-invite-link
GROUP_ID=your-test-group-id
FOR_LOGS=your-test-logs-chat-id

# Платежные системы (тестовые)
YOKASSA_ACCOUNT_ID=
YOKASSA_SECRET_KEY=

# Rubitime API (тестовые)
RUBITIME_API_KEY=
RUBITIME_BASE_URL=https://rubitime.ru/api2/
RUBITIME_BRANCH_ID=12595
RUBITIME_COOPERATOR_ID=25786

# Внешние URL
ADMIN_URL=https://t.me/partacoworking
RULES_URL=https://parta-works.ru/main_rules

# Порты для локального использования
PORT=8000
FRONTEND_PORT=80

# Лимиты ресурсов (минимальные для разработки)
REDIS_MAXMEMORY=256mb

# Кэширование (короткие TTL для разработки)
CACHE_DEFAULT_TTL=300

# Логирование (детальное для разработки)
LOG_LEVEL=DEBUG

# Backup (отключено для разработки)
BACKUP_ENABLED=false