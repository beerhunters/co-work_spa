# Домен для SSL сертификата
DOMAIN_NAME=your-domain.com
SSL_EMAIL=your-email@example.com

# Рабочая директория проекта (автоматически заменится скриптом установки)
COWORKING_DIR=/opt/coworking

# Основные настройки
DEBUG=false
ENVIRONMENT=production
APP_NAME=Coworking API
APP_VERSION=1.0.0

# Безопасность (ЗАПОЛНИ СВОИМИ ЗНАЧЕНИЯМИ!)
SECRET_KEY=your-super-secret-key-here
SECRET_KEY_JWT=your-jwt-secret-key-here
ADMIN_LOGIN=admin
ADMIN_PASSWORD=secure-admin-password

# Telegram Bot (ЗАПОЛНИ СВОИМИ ЗНАЧЕНИЯМИ!)
BOT_TOKEN=your-bot-token
ADMIN_TELEGRAM_ID=your-admin-id
BOT_LINK=https://t.me/your_bot
INVITE_LINK=your-invite-link
GROUP_ID=your-group-id
FOR_LOGS=your-logs-chat-id

# Платежные системы (ЗАПОЛНИ ЕСЛИ ИСПОЛЬЗУЕШЬ)
YOKASSA_ACCOUNT_ID=
YOKASSA_SECRET_KEY=

# Rubitime API (ЗАПОЛНИ ЕСЛИ ИСПОЛЬЗУЕШЬ)
RUBITIME_API_KEY=
RUBITIME_BASE_URL=https://rubitime.ru/api2/
RUBITIME_BRANCH_ID=12595
RUBITIME_COOPERATOR_ID=25786

# Внешние URL
ADMIN_URL=https://t.me/partacoworking
RULES_URL=https://parta-works.ru/main_rules

# Лимиты ресурсов
WEB_MEMORY_LIMIT=1G
WEB_MEMORY_RESERVATION=512M
REDIS_MEMORY_LIMIT=512M
REDIS_MEMORY_RESERVATION=256M
REDIS_MAXMEMORY=512mb

# Кэширование
CACHE_DEFAULT_TTL=600
CACHE_DASHBOARD_TTL=120
CACHE_USER_DATA_TTL=1200
CACHE_STATIC_DATA_TTL=3600

# Логирование
LOG_LEVEL=INFO
TELEGRAM_LOGGING_ENABLED=true

# Backup
BACKUP_ENABLED=true
BACKUP_INTERVAL_HOURS=6